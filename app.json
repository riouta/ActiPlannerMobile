{
    "expo": {
      "name": "ActiPlannerMobile",
      "slug": "ActiPlannerMobile",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./app/assets/images/react_logo.png",
      "scheme": "myapp",
      "userInterfaceStyle": "automatic",
      "splash": {
        "image": "./app/assets/images/react_logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "ios": {
        "supportsTablet": true
      },
      "android": {
        "adaptiveIcon": {
          "foregroundImage": "./app/assets/images/react_logo.png",
          "backgroundColor": "#ffffff"
        }
      },
      "web": {
        "bundler": "metro",
        "output": "static",
        "favicon": "./app/assets/images/react_logo.png"
      },
      "plugins": [
        "expo-router"
      ],
      "experiments": {
        "typedRoutes": true
      }
    }
  }
  